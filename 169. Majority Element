class Solution:
    def majorityElement(self, nums: List[int]) -> int:
        for i in set(nums) :
            if nums.count(i)  > len(nums)//2 :
                return i
                
                
                
                
                
                
class Solution:
    def majorityElement(self, nums: List[int]) -> int:
        size = len(nums)
        nums.sort()
        return nums [int (size/2)]
