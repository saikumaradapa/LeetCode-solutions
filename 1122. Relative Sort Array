class Solution:
    def relativeSortArray(self, arr1: List[int], arr2: List[int]) -> List[int]:
        arr = [0] * 1001

        for i in range(len(arr1)):
            arr[arr1[i]] += 1 
        
        res = []

        for i in range(len(arr2)):            
            res.extend([arr2[i]]*arr[arr2[i]])
            arr[arr2[i]] = 0
        
        for i in range(1001):
            if arr[i]:
                res.extend([i] * arr[i])
        
        return res

''' time complexity : O(n)        
    space complexity : O(1001)
'''
