class Solution:
    def longestCommonPrefix(self, strs: List[str]) -> str:
        result = ''
        strs.sort()
        i = 0
        a = strs[0]
        b = strs[-1]
        while i < len(a) :
            if a[i] != b[i]:
                break
            result += a[i]
            i += 1

        return result
