class Solution:
    def hammingWeight(self, n: int) -> int:
        res = 0 
        for i in range(32) :
            if  n & 1 :
                res += 1 
            n >>= 1
        return res
        
        
        
class Solution:
    def hammingWeight(self, n: int) -> int:
        return len([num for num in str(bin(n)) if num =='1'])
